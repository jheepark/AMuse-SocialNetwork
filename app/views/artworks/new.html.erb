<div class="photoheading">
  <h2>Upload your artwork</h2>
</div>
<div class="blackbox">
  <%= form_for @artwork, :html => {:multipart => true} do |f| %>
  <div class="editform">
    <%= hidden_field_tag :photo_id, params[:photo_id]%>
    <fieldset>
      <%= f.label :image, "Image URL" %>
      <br>
      <%= f.file_field :image%>
    </fieldset>
    <%= f.submit "Upload!", :class => "submit" %>
    <% end %>
  </div>
